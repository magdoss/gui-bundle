{% extends 'WebProfilerBundle:Profiler:layout.html.twig' %}

{% block toolbar %}
    <div class="sf-toolbar-block">
        <div class="sf-toolbar-icon">
            <a href="{{ path('gui') }}">
                <img width="28" height="28" alt="Rapid Development Tools" src="data:image/png;base64,">
            </a>
        </div>
        <div class="sf-toolbar-info">
            <div class="sf-toolbar-info-piece">
                <a href="{{ path('gui') }}"><strong>Rapid Development Tools</strong></a>
            </div>
            <div class="sf-toolbar-info-piece">
                <a href="{{ path('guiCreateController') }}"><span class="glyphicon glyphicon-file"></span> Page Generator</a>
            </div>
            <div class="sf-toolbar-info-piece">
                <a href="{{ path('guiCreateCrud') }}"><span class="glyphicon glyphicon-wrench"></span> Entity Generator</a>
            </div>
            <div class="sf-toolbar-info-piece">
                <a href="{{ path('guiCreateBundle') }}"><span class="glyphicon glyphicon-briefcase"></span> Bundle Generator</a>
            </div>
            <div class="sf-toolbar-info-piece">
                <a href="{{ path('guiInstallBundle') }}"><span class="glyphicon glyphicon-cloud-download"></span> Bundle Installer</a>
            </div>
            <div class="sf-toolbar-info-piece">
                <a href="{{ path('guiCreateForm') }}"><span class="glyphicon glyphicon-list-alt"></span> Form Generator</a>
            </div>
            <div class="sf-toolbar-info-piece">
                <a href="{{ path('guiCreateStyle') }}"><span class="glyphicon glyphicon-tint"></span> Theme Generator</a>
            </div>
        </div>
    </div>
{% endblock %}

